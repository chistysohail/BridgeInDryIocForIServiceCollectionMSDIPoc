<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
	<ItemGroup>
		<!-- NuGet package for DryIoc -->
		<PackageReference Include="DryIoc.dll" Version="4.7.0" />
		<!-- NuGet package for integrating DryIoc with Microsoft.Extensions.DependencyInjection -->
		<PackageReference Include="DryIoc.Microsoft.DependencyInjection" Version="5.0.0" />
		<!-- Microsoft.Extensions.DependencyInjection for DI abstractions -->
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.0" />
	</ItemGroup>
</Project>
